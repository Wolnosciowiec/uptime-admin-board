{
    "name": "riotkit-org/riothealthflux",
    "type": "project",
    "license": "Apache-2.0",
    "require": {
        "ext-json": "*",
        "ext-curl": "*",
        "php-di/php-di": "^6.0",
        "symfony/console": "^5.2",
        "monolog/monolog": "^2.1",
        "influxdb/influxdb-php": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Riotkit\\HealthFlux\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload -o"
        ]
    }
}
